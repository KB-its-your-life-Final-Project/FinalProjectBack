<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lighthouse.wishlist.mapper.LawdCdMapper">
    <select id="findUmdNmByRegionCd" resultType="String">
        SELECT locallow_nm
        FROM ${API_LAWD_CD}
        WHERE region_cd = #{regionCd}
    </select>
</mapper>
